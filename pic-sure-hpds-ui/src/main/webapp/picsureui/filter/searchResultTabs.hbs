<div class="filter-search row">
    <ul class="nav nav-pills">
    	
        {{#each aliases}}
	        <li data-toggle="tooltip" id="{{this.alias}}-pill"><a title="{{this.tooltip}}" data-target="#{{../filterId}}  #{{this.alias}}" data-toggle="tab" aria-expanded="true">{{this.alias}}</a></li>
        {{/each}}
    </ul>

    <div class="tab-content">
	    	{{#each aliases}}
			<div class="tab-pane" id="{{this.alias}}">
				<div class="result-subcategories-div"></div>
				<div class="search-result-list"></div>
			</div>
		{{/each}}
	</div>

</div>
