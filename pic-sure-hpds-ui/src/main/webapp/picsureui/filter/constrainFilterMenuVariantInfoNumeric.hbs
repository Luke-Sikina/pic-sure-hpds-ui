<div class="constrain-row row">
    <div class="col-md-4 constrain-filter-btn">
    </div>
        <div class="checkbox-list">
            <label>Min</label><input class="constrain-value constrain-value-one form-control" value="{{constrainValueOne}}" type="number"></input><span class="value-range-hint">(default : {{metadata.min}})</span><br>
            <label>Max</label><input class="constrain-value constrain-value-two form-control" value="{{constrainValueTwo}}" type="number"></input><span class="value-range-hint">(default : {{metadata.max}})</span>
        </div>
    <div class="constrain-search-button col-md-1">
        <span class="btn constrain-apply-btn">Run</span>
    </div>
    <div class="validation-message col-md-12"></div>
</div>
